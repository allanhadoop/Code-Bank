/// Linux: md5sum filename or sha256sum filename

///upgrade to python3.5 
yum install gcc
cd /usr/src
sudo wget https://www.python.org/ftp/python/3.5.2/Python-3.5.2.tgz
sudo tar xzf Python-3.5.2.tgz
cd Python-3.5.2
sudo ./configure
sudo make altinstall
python3.5 -V

//Anaconda - 
Download anaconda from https://www.continuum.io/downloads , transfer it to EC2
Check md5sum <filename-path>
Check if key matches with https://repo.continuum.io/archive/
sudo yum install bzip2 
now run sh <filename.sh>

conda --version
python --version

//For python3.5 use - bash Anaconda3-4.2.0-Linux-x86_64.sh
//For python2.7 use - bash Anaconda2-4.2.0-Linux-x86_64.sh 
